[Unit]
  Description=RNBO OSCQuery Service
  StartLimitIntervalSec=500
  StartLimitBurst=5
  StartLimitInterval=0
  After=pipewire.service
  BindsTo=pipewire.service
  ConditionUser=!root

[Service]
  Type=idle
  ExecStart=pw-jack /usr/bin/rnbooscquery
  KillSignal=SIGINT
  Restart=on-failure
  RestartSec=5s

[Install]
  WantedBy=default.target
