<!DOCTYPE node PUBLIC
    "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
    "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd" >
<node xmlns:doc="http://www.freedesktop.org/dbus/1.0/doc.dtd">
  <interface name="com.cycling74.rnbo">
    <method name="QueueRunnerInstall">
      <arg name="version" direction="in" type="s">
        <doc:doc><doc:summary>The version string of the package to install.</doc:summary></doc:doc>
      </arg>
      <doc:doc>
        <doc:description>
          <doc:para>
            Queues a task to install a specific version of the rnbooscquery runner.
            The status of that task should be reflected in the properties.
          </doc:para>
        </doc:description>
      </doc:doc>
    </method>

    <property name="Active" type="b" access="read">
      <doc:doc>
        <doc:description>
          <doc:para>
            Indicates if the update service is actively updating something.
          </doc:para>
        </doc:description>
      </doc:doc>
    </property>
    <property name="Status" type="s" access="read">
      <doc:doc>
        <doc:description>
          <doc:para>
            Indicates the last status string of the installation.
          </doc:para>
        </doc:description>
      </doc:doc>
    </property>

  </interface>
</node>
